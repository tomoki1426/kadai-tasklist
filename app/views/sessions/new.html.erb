<h1>ログイン</h1>
<%= form_with(url: login_path, scope: :session, local: true) do |f| %>


         <%= f.label :email, 'Email' %>
         <%= f.email_field :email %>

     
         <%= f.label :password, 'Password' %>
         <%= f.password_field :password %>
    
          <%= f.submit 'ログイン' %>
    <% end %>

    <p>New user? <%= link_to '新規登録', signup_path %></p>